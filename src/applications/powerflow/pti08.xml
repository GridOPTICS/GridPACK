    <LinearSolver>
        <PETScPrefix>nrs</PETScPrefix>
        <PETScOptions>
          -ksp_type bicg
          -pc_type bjacobi
          -sub_pc_type ilu -sub_pc_factor_levels 11 sub_ksp_type preonly 
          -ksp_atol 1.0e-15
          -ksp_rtol 1.0e-10
          -ksp_monitor
          -ksp_max_it 10000
          -ksp_view
        </PETScOptions>
    </LinearSolver>
